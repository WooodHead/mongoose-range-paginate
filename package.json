{
  "name": "mongoose-range-paginate",
  "version": "0.0.4",
  "description": "Fast range paginator for mongoose",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test.js --reporter=spec",
    "test:coverage": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha test.js -- --reporter=spec",
    "test:travis": "npm run test:coverage && npm run coveralls",

    "mongodb"
  ],
  "author": "Kilian Ciuffolo <me@nailik.org> (http://nailik.org)",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.9.1",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "mocha": "^1.21.0",
    "mongoose": "^3.8.13"
  }
}
